<div id="bulk-add-to-map-modal" class="add-to-map-modal" data-components="bulkAddMapManager">
  <header><h2><%= t 'geo_data.bulk_add_map.title' %></h2></header>
  <section>
    <div class="explanation"><%=  t 'geo_data.bulk_add_map.explanation' %></div>
    <%= form_tag bulk_add_map_geo_data_index_path, method: :post, id: "add-to-map-form", remote: true, "data-components" => "remoteForm" do %>
      <p>
        <%= autocomplete_field_tag :map, search_by_name_maps_path %>
      </p>

      <p class="btns">
        <%= submit_tag t('save'), class: "submit-btn" %>
      </p>
    <% end %>
  </section>
</div>

