<%= render 'shared/toolbar', :object => object, :only => tools(object) %>

<div class="site-page <%= "#{ params[:controller] } #{ params[:action] }" %>" >

  <%= remote_form_for @geo_data do |f| %>

    <%= render 'shared/form_actions', object: object %>

    <%= render header_partial, f: f, name => object %>
    <%= render 'shared/objects/location', object: object %>

    <%= render content_partial, f: f, name => object %>
  <% end %>
</div>
