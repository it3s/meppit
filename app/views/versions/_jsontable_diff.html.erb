
<div class='jsontable-before'>
  <div class='jsontable-title'><%= t 'before'%>:</div>
  <% if vals.old_value %>
    <div class="json-table" data-components="jsonTable"
      data-component-jsontable='<%= additional_info_json OpenStruct.new(additional_info: vals.old_value) %>'></div>
  <% else %>
    <div class="jsontable-empty"><%= t 'versions.jsontable.empty' %></div>
  <% end %>
</div>

<div class='jsontable-after'>
  <div class='jsontable-title'><%= t 'after' %>:</div>
  <% if vals.new_value %>
    <div class="json-table" data-components="jsonTable"
      data-component-jsontable='<%= additional_info_json OpenStruct.new(additional_info: vals.new_value) %>'></div>
    <% else %>
      <div class="jsontable-empty"><%= t 'versions.jsontable.empty' %></div>
    <% end %>
</div>
